{
    "name": "artisanry/cookie-consent",
    "description": "Easy drop-in Cookie Consent (Silktide) for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Cookie-Consent", "Laravel CookieConsent"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\CookieConsent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\CookieConsent\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\CookieConsent\\CookieConsentServiceProvider"
            ],
            "aliases": {
                "CookieConsent": "Artisanry\\CookieConsent\\Facades\\CookieConsent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
